	note = {Conference Name: IEEE Systems Journal},
	keywords = {Androids, Bluetooth, Bluetooth Low Energy (BLE) beacons, Estimation, Internet of Things, Kalman filter, Kalman filters, Received signal strength indicator, Wireless communication, iBeacons, indoor positioning, received signal strength indicator (RSSI), smart museum},
