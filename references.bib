@article{du_kf-knn_2020,
	title = {{KF}-{KNN}: {Low}-{Cost} and {High}-{Accurate} {FM}-{Based} {Indoor} {Localization} {Model} via {Fingerprint} {Technology}},
	volume = {8},
	issn = {2169-3536},
	shorttitle = {{KF}-{KNN}},
	url = {https://ieeexplore.ieee.org/abstract/document/9234441},
	doi = {10.1109/ACCESS.2020.3031089},
	abstract = {Localization and tracking of personnel and equipment are technical issues that urgently need to be solved for Indoor positioning. To improve the accuracy and environmental adaptability of personnel and equipment localization algorithms in the construction and operation of smart water platform, this paper proposes a fingerprint localization algorithm (KF-KNN) based on FM signals. Firstly, use FM data collection device to obtain RSSI fingerprint information within the coverage area, and train them to build a fingerprint database; secondly, KNN technology is used to complete the rough localization calculation based on the RSSI data received by the module to be located, the RSSI fingerprint database and environmental noise parameters; finally, the Kalman filter model is used to predict and optimize the rough position information, so as to have better environmental adaptability and effectively improve the accuracy of localization. The analysis results show that: compared with the original KNN and WKNN fingerprint localization models, the KF-KNN algorithm has better performance in localization, and its average localization error can be as low as 1.9 meters.},
	urldate = {2024-10-12},
	journal = {IEEE Access},
	author = {Du, Canyang and Peng, Bao and Zhang, Zhaobo and Xue, Weicheng and Guan, Mingxiang},
	year = {2020},
	note = {Conference Name: IEEE Access},
	keywords = {Databases, Fingerprint, Fingerprint recognition, Frequency modulation, KNN, Kalman filter model, Personnel, Prediction algorithms, Received signal strength indicator, Training, localization},
	pages = {197523--197531},
}

@article{cai_museum_2015,
	title = {Museum {Navigation} based on {NFC} {Localization} {Approach} and {Automatic} {Guidance} {System}},
	volume = {120},
	issn = {09758887},
	url = {http://research.ijcaonline.org/volume120/number1/pxc3903859.pdf},
	doi = {10.5120/21188-3859},
	abstract = {This paper presents an automatic museum guide system that provides both interactive guidance for exhibition and a NFC based location navigation. It is well known that NFC is a kind of short distance communication for portable terminals such smart phones and tablet devices. The communication area of NFC is limited in a narrow area; therefore, NFC may be used to locate the position of the area. To realize the automatic guidance and navigation, we present a new data architecture to contain the location information, and an evaluation function to calculate the optical exhibition candidates for the visitors. The feature of the function is considering the popularity of the exhibition and movement distance between the exhibition spots. To implement the service, we developed an application to support editing the map, and create the map data for the portable terminals. It is conﬁrmed that the system provide a good performance based on the experiment in museum.},
	language = {en},
	number = {1},
	urldate = {2024-10-12},
	journal = {International Journal of Computer Applications},
	author = {Cai, Dawei},
	month = jun,
	year = {2015},
	pages = {1--7},
}

